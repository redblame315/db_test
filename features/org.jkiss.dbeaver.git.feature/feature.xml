<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.jkiss.dbeaver.git.feature"
      label="DBeaver Git support"
      version="1.0.157.qualifier"
      provider-name="DBeaver Corp">

   <copyright>DBeaver Corp and others</copyright>
   <license url="https://dbeaver.io/product/dbeaver_license.txt"/>

   <requires>
      <import feature="org.jkiss.dbeaver.ui.feature" version="0.0.0" match="greaterOrEqual"/>
   </requires>

   <includes id="org.eclipse.jgit" version="0.0.0"/>
   <includes id="org.eclipse.jgit.gpg.bc" version="0.0.0"/>
   <includes id="org.eclipse.jgit.http.apache" version="0.0.0"/>
   <includes id="org.eclipse.jgit.ssh.apache" version="0.0.0"/>
   <includes id="org.eclipse.egit" version="0.0.0"/>

   <!-- SSH Agent provider -->
   <plugin id="org.apache.sshd.osgi" version="0.0.0"/>
   <plugin id="org.apache.sshd.sftp" version="0.0.0"/>

   <!-- DBeaver deps -->
   <plugin id="org.jkiss.dbeaver.team.git.ui" version="0.0.0"/>

</feature>
